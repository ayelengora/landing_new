<section class="site-section p-5" id="contact-section">
      <div id="contacto" class="container-fluid align-items-stretchcol-12 bg-light w-100 ft-feature-1-content px-5 py-4  p-5 shadow-sm p-3 mb-5 bg-light rounded">
        <div class="row  mb-5">
          <div class="col-12 text-center ">
            <h2 class="text-black h1 site-section-heading text-large">Contáctenos</h2>
          </div>
        </div>
        <div class="row">
          <div class="col-md-7 mb-5">

            

            <form action="#" class="p-5 bg-light">
              
              <h2 class="h4 text-black mb-5">Formulario de contacto</h2> 

              <div class="row form-group">
                <div class="col-md-6 mb-3 mb-md-0">
                  <label class="text-black" for="fname">Nombre</label>
                  <input type="text" id="fname" class="form-control rounded-pill">
                </div>
                <div class="col-md-6">
                  <label class="text-black" for="lname">Apellido</label>
                  <input type="text" id="lname" class="form-control rounded-pill">
                </div>
              </div>

              <div class="row form-group">
                
                <div class="col-md-12">
                  <label class="text-black" for="email">Correo electrónico</label> 
                  <input type="email" id="email" class="form-control rounded-pill">
                </div>
              </div>

              <div class="row form-group">
                
                <div class="col-md-12">
                  <label class="text-black" for="subject">Tema</label> 
                  <input type="subject" id="subject" class="form-control rounded-pill">
                </div>
              </div>

              <div class="row form-group">
                <div class="col-md-12">
                  <label class="text-black" for="message">Mensaje</label> 
                  <textarea name="message" id="message" cols="30" rows="7" class="form-control rounded" placeholder="Escriba sus notas o preguntas aquí..."></textarea>
                </div>
              </div>

              <div class="row form-group">
                <div class="col-md-12">
                  <input type="submit" value="Enviar" class="btn btn-outline-dark btn-lg">
                </div>
              </div>

  
            </form>
          </div>
          <div class="col-md-5">
            
            <div class="p-4 mb-3 bg-light">
              <p class="mb-0 font-weight-bold">Dirección</p>
              <p class="mb-4">Devoto, Buenos Aires Argentina</p>

              <p class="mb-0 font-weight-bold">Telefono</p>
              <p class="mb-4"><a href="#">+54 9 11 45661-3251</a></p>

              <p class="mb-0 font-weight-bold">Dirección</p>
              <p class="mb-0"><a href="#">info@viewtalent.com</a></p>

            </div>
            
          </div>
        </div>
      </div>
    </section>
